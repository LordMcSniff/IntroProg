{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++ build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-std=c11",
				"-Wall",
				"-Werror",
				"${workspaceRoot}/*.c",
				"-o",
				"${workspaceRoot}/out.o"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		},
		{
			"type": "cppbuild",
			"label": "build test",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-std=c11",
				// "-Wall",
				"-lm",
				"${workspaceRoot}/main.c",
				"-o",
				"${workspaceRoot}/main"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build"
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}